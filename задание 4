#include <iostream>
using namespace std;
int main()
{
	setlocale(0, "");
	int arr[] = { 3, -5, 55, -46, 5, -3 };
	int size = sizeof(arr) / sizeof(arr[0]);

	for (int i = 0; i < size; ++i)
	{
		if (arr[i] > 0)
		{
			cout << "положитьельные числа:" << arr[i]<<"\n";
		}
	}
	
	for (int i = 0; i < size; ++i)
	{
		if (arr[i] < 0)
		{
			cout << "\nотрицательные числа:" << arr[i];
		}
	}

	return 0;
}
